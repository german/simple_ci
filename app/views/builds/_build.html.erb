<div class="row <%= last_build ? 'last_build' : '' %>">
  <div class="small-4 columns <%= build.failure? ? 'alert' : 'success' %> label">build #<%= build.id %></div>
  <div class="small-4 columns"><%= build.aasm_state %></div>
  <div class="small-4 columns">run <%= time_ago_in_words build.updated_at %> ago</div>
</div>